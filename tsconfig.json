{
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es2018",
        "noImplicitAny": false,
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "dist",
        "baseUrl": "./src",
        "paths": {
            ".": [
                "node_modules/*",
            ]
        },
        "skipLibCheck": true
    }
}